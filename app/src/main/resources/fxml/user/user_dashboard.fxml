<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" 
      fx:controller="com.gameaffinity.view.UserDashboardView"
      spacing="10" alignment="CENTER">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <Button text="View Library" fx:id="viewLibraryButton" />
    <Button text="Manage Friends" fx:id="manageFriendsButton" />
    <Button text="View Game Database" fx:id="viewGameDatabaseButton" />
    <Button text="Modify Profile" fx:id="modifyProfileButton" />
    <Button text="Logout" fx:id="logoutButton" />
</VBox>
